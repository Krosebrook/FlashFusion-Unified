{
  "dashboard": {
    "id": null,
    "title": "FlashFusion Security & Performance Dashboard",
    "tags": ["flashfusion", "security", "performance", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "flashfusion_health_status",
            "legendFormat": "Health Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Security Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(flashfusion_security_events_total[5m])",
            "legendFormat": "{{event_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(flashfusion_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(flashfusion_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Rate Limiting Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(flashfusion_rate_limit_exceeded_total[5m])",
            "legendFormat": "Rate Limit Exceeded"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(flashfusion_database_operation_duration_seconds_sum[5m]) / rate(flashfusion_database_operation_duration_seconds_count[5m])",
            "legendFormat": "Avg Query Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Redis Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(flashfusion_redis_operation_duration_seconds_sum[5m]) / rate(flashfusion_redis_operation_duration_seconds_count[5m])",
            "legendFormat": "Avg Redis Operation Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Failed Authentication Attempts",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(flashfusion_auth_failures_total[5m])",
            "legendFormat": "Failed Logins"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "flashfusion_active_users",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Total API Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(flashfusion_http_requests_total[1h])",
            "legendFormat": "Requests per Hour"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 11,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(flashfusion_http_requests_total{status=~\"5..\"}[5m]) / rate(flashfusion_http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 32}
      },
      {
        "id": 12,
        "title": "Database Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "flashfusion_database_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 32}
      },
      {
        "id": 13,
        "title": "Security Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "flashfusion_security_alerts",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      },
      {
        "id": 14,
        "title": "Top API Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(flashfusion_http_requests_total[5m])) by (endpoint))",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 15,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "links": [],
    "annotations": {
      "list": []
    },
    "templating": {
      "list": []
    }
  }
} 